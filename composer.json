{
    "name": "dracoblue/craur",
    "description": "A lossless xml to json and json to xml converter (and csv/xlsx/yaml). Writing PHP Json/Xml/Csv/Yaml/excel Importers made easy",
    "keywords": [ "importer", "exporter", "dom", "xml", "json", "csv", "yaml", "excel" ],
    "license": "MIT",
    "authors": [
        {
            "name": "DracoBlue",
            "email": "JanS@DracoBlue.de"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "classmap": [ "src/" ]
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.pearplex.net/",
            "vendor-alias": "pearplex"
        }
    ],
    "require": {
        "dracoblue/naith": "1.1.*",
        "pearplex/PHPExcel": "*",
        "symfony/yaml": "*"
    },
    "config": {
        "bin-dir":          "bin/"
     },
    "bin": [ "bin/craur" ]
}
